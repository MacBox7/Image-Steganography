{
    "extends": "google",
    "globals": {
        "document": true,
        "foo": true,
        "window": true,
        "TweenMax": true,
        "Linear": true,
        "Back": true,
        "$": true
    },
    "no-use-before-define": ["error", {
        "functions": false,
        "classes": false
    }],
    "rules": {
        "no-var": 0,
        "require-jsdoc": 0,
        "max-len": "off",
        "space-before-function-paren": "off",
        "eol-last": "off"
    }
}